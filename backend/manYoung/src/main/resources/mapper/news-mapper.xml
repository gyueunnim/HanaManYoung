<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="News">
    <select id="selectNewsList" resultType="News" parameterType="String">
        select * from hana.news where news_type=#{news_type}
    </select>
</mapper>